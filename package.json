{
  "name": "sen-sage",
  "version": "0.0.4",
  "description": "Answer all the common questions about tokens on Solana",
  "author": {
    "name": "Tu Phan",
    "email": "tuphan@descartes.network"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "concurrently --names LINT,DEV -c yellow,cyan \"pnpm lint\" \"NODE_ENV=development nest start --watch\"",
    "start": "NODE_ENV=production node dist/main",
    "lint": "esw \"{src,test}/**/*.ts\" --watch --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "engines": {
    "node": ">=16 <=18"
  },
  "resolutions": {
    "@solana/buffer-layout": "^4.0.1"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.28.0",
    "@coral-xyz/spl-token": "^0.28.0",
    "@metaplex-foundation/js": "^0.20.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/throttler": "^5.0.1",
    "@solana/spl-token-registry": "^0.2.4574",
    "@solana/web3.js": "^1.78.0",
    "@types/cache-manager": "^4.0.6",
    "axios": "^1.4.0",
    "bn.js": "^5.2.1",
    "cache-manager": "^4.1.0",
    "cache-manager-fs-hash": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "gifencoder": "^2.0.1",
    "ip": "^1.1.8",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bn.js": "^5.1.1",
    "@types/cache-manager-fs-hash": "^0.0.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/gifencoder": "^2.0.1",
    "@types/ip": "^1.1.0",
    "@types/jest": "29.2.4",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-watch": "^8.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
